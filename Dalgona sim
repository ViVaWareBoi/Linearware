local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("LinearHub V3 | Dalgona simulator", "Synapse")

local Tab = Window:NewTab("Autofarm")

local Bp = Tab:NewSection("Autofarm")

Bp:NewToggle("Fast farm (Triangle dalgona)", "Farm Triangle dalgona", function(state)
    if state then
        

getgenv().autofarm = true

while autofarm == true do
local args = {
  [1] = "Triangle"
}
game:GetService("ReplicatedStorage").Remotes.Win:FireServer(unpack(args))
local args = {
  [1] = 1
}
game:GetService("ReplicatedStorage").Remotes.Award:FireServer(unpack(args))
 wait()
end
    else
        getgenv().autofarm = false
    end
end)


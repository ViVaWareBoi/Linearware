	if Keybox.Text == "XR:m&Qr=9(J=F4r^" then
		Check.Text = "Correct Key!"
		Check.TextColor3 = Color3.fromRGB(0, 255, 0)
		
		wait(1)
		
		WhitelistBox.Visible = false
		
		wait(0.9)
		
		LoadCOntainer.Visible = false
                

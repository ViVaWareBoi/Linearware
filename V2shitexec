-- Scripts:

local function NAJC_fake_script() -- LoadCOntainer.LocalScript 
	local script = Instance.new('LocalScript', LoadCOntainer)

	script.Parent:TweenPosition(UDim2.new(0.29, 0, 0.315, 0))
	
	wait(1)
	
	Status.Text = "Checking Key..."
	
	wait(1)
	
	Status.Text = "Key not found!"
	
	wait(1)
	
	Status.Text = "Please get a new key"
	
	wait(0.8)
	
	WhitelistBox.Visible = true
end
coroutine.wrap(NAJC_fake_script)()

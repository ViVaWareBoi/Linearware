Get.MouseButton1Down:connect(function()
	setclipboard("https://pastebin.com/raw/LMaDAwDu")
	local CoreGui = game:GetService("StarterGui")

	CoreGui:SetCore("SendNotification", {
		Title = "Balls";
		Text = "Copied to clipboard";
		Duration = 5;
		--Callback here
		Button1 = "Ok";
		Button2 = "Bells";
	})
end)

